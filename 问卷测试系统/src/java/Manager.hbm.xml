<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.qdu.pojo.Manager" table="Manager">
        <id name="managerNo" type="integer">
            <column name="ManagerNo" />
        </id>
        <property name="managerPassword" type="string">
            <column name="ManagerPassword"/>
        </property>
        <property name="managerName" type="string">
            <column name="ManagerName"/>
        </property>
        <property name="managerGender" type="string">
            <column name="ManagerGender"/>
        </property>
        <property name="managerMobile" type="string">
            <column name="ManagerMobile"/>
        </property>
    </class>
</hibernate-mapping>
